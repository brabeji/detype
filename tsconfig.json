{
	"compilerOptions": {
		"noEmit": true,
		"strict": true,
		"module": "ES2020",
		"lib": ["ES2020", "ES2021.String"],
		"esModuleInterop": true,
		"target": "es6",
		"moduleResolution": "node",
		"types": ["node"],
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"noUncheckedIndexedAccess": true,
		"checkJs": true
	},
	"exclude": ["node_modules", "dist", "detype.js"]
}
